<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Kenan Nur</Authors>
    <Owners>Kenan Nur</Owners>
    <Title>Vegas.AspNetCore.Logger</Title>
    <RepositoryUrl>https://github.com/kenannur/Vegas.AspNetCore.Extensions</RepositoryUrl>
    <PackageId>Vegas.AspNetCore.Logger</PackageId>
    <PackageVersion>1.0.2</PackageVersion>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Serilog\" />
    <None Remove="Serilog.AspNetCore" />
    <None Remove="Serilog.Sinks.File" />
    <None Remove="AWS.Logger.SeriLog" />
    <None Remove="Amazon\" />
    <None Remove="Amazon.Lambda.Logging.AspNetCore" />
    <None Remove="AWS.Logger.AspNetCore" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Serilog\" />
    <Folder Include="Amazon\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Serilog.AspNetCore" Version="4.1.0" />
    <PackageReference Include="Serilog.Sinks.File" Version="5.0.0" />
    <PackageReference Include="AWS.Logger.SeriLog" Version="3.1.1" />
    <PackageReference Include="Amazon.Lambda.Logging.AspNetCore" Version="3.1.0" />
    <PackageReference Include="AWS.Logger.AspNetCore" Version="3.2.0" />
  </ItemGroup>
</Project>